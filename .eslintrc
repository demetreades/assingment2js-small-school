{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "rules": {
        "class-methods-use-this": "off",
        "no-underscore-dangle": "off",
        "comma-dangle": "off",
        "no-console": "off",
        "operator-linebreak": "off"
    }
}